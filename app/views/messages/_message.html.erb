<div id="<%= dom_id message %>">
  <p>
    <strong>Name:</strong>
    <%= message.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= message.email %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= message.message %>
  </p>

  <p>
    <strong>Read:</strong>
    <%= message.read %>
  </p>

</div>
